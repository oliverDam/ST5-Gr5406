journal = {Pain},
keywords = {SAF,central sensitization,clinical pain,cytokines,inflammation,laboratory pain,qst,sickle cell disease},
mendeley-tags = {SAF},
number = {4},
pages = {949--956},
title = {{Quantitative sensory testing and pain-evoked cytokine reactivity: Comparison of patients with sickle cell disease to healthy matched controls}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84973345835{\&}partnerID=40{\&}md5=6581568912279e2da602c981f2b71a7a},
