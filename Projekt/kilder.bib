keywords = {Chronic Disease,Humans,Logistic Models,Pain,Pain Measurement,Pain: diagnosis,Pain: physiopathology,Postoperative,Postoperative: diagnosis,Predictive Value of Tests,QST,f{\o}r og efter,operation,smerte},
