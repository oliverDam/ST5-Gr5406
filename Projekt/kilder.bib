@article{Nielsen2009,
abstract = {Backgrounds and Aims: Quantitative testing of patients' basal preoperative pain perception may be of clinical value if it accurately can predict magnitude of pain and requirement of analgesics after surgery. This article is a review of studies that have investigated the correlation between preoperative responses to experimental pain stimuli and clinical postoperative pain. The objective was to examine the predictive, statistical potential of these experimental pain stimuli and to evaluate their application in a scientific and clinical context. Methods: Studies were identified through systematic search in relevant databases. Results: Thirteen relevant studies were retrieved. Experimental stimulation methods were the cold-pressor test (n = 1), heated water immersion test (n = 1), phasic (n = 4), or tonic (n = 1) heat stimulation, cutaneous electrical stimulation (n = 4), pressure algometry (n = 1), punctate mechanical stimulation (n = 1), and induction of an inflammatory injury (n = 1). Pain sensitivity was evaluated by threshold (n = 12) or suprathreshold stimuli (n = 8), DNIC efficiency (n = 1), and/or pain tolerance (n = 3). The preoperative experimental tests could account for 5{\%} to 43{\%} of the variance in postoperative pain experience. Two of the studies indicated that an increase in preoperative pain sensitivity was associated with development of chronic pain following surgery. Conclusions: Preoperative psychophysical assessments methods are valuable tools in prediction of postoperative acute and chronic pain.},
author = {Mj{\"{o}}bo, H N and Rudin, A. and Nielsen, P R and Werner, M U},
file = {:Volumes/Macintosh HD/Downloads/0000542-201006000-00030.pdf:pdf},
issn = {1530-7085},
journal = {Pain Practice},
keywords = {algometry,analgesic agent,chronic pain,cold pressor test,data base,electrostimulation,experimental test,heat,injury,mechanical stimulation,nociception,pain,pain threshold,patient,postoperative pain,prediction,stimulation,stimulus,surgery,systematic review,water immersion},
number = {6},
pages = {148--149},
title = {{Postoperative pain prediction: A systematic review of preoperative experimental pain studies}},
url = {http://www.embase.com/search/results?subaction=viewrecord{\&}from=export{\&}id=L70207406$\backslash$nhttp://dx.doi.org/10.1111/j.1533-2500.2009.00267.x$\backslash$nhttp://sfx.library.uu.nl/utrecht?sid=EMBASE{\&}issn=15307085{\&}id=doi:10.1111/j.1533-2500.2009.00267.x{\&}atitle=Postoperative+},
volume = {9},
year = {2009}
}
@article{Moreton2015,
abstract = {OBJECTIVE: Multiple mechanisms are involved in pain associated with osteoarthritis (OA). The painDETECT and Self-Report Leeds Assessment of Neuropathic Symptoms and Signs (S-LANSS) questionnaires screen for neuropathic pain and may also identify individuals with musculoskeletal pain who exhibit abnormal central pain processing. The aim of this cross-sectional study was to evaluate painDETECT and S-LANSS for classification agreement and fit to the Rasch model, and to explore their relationship to pain severity and pain mechanisms in OA.$\backslash$n$\backslash$nMETHODS: A total of 192 patients with knee OA completed questionnaires covering different aspects of pain. Another group of 77 patients with knee OA completed questionnaires and underwent quantitative sensory testing for pressure-pain thresholds (PPTs). Agreement between painDETECT and S-LANSS was evaluated using kappa coefficients and receiver operator characteristic (ROC) curves. Rasch analysis of both questionnaires was conducted. Relationships between screening questionnaires and measures of pain severity or PPTs were calculated using correlations.$\backslash$n$\backslash$nRESULTS: PainDETECT and S-LANSS shared a stronger correlation with each other than with measures of pain severity. ROC curves identified optimal cutoff scores for painDETECT and S-LANSS to maximize agreement, but the kappa coefficient was low ($\kappa$ = 0.33-0.46). Rasch analysis supported the measurement properties of painDETECT but not those of S-LANSS. Higher painDETECT scores were associated with widespread reductions in PPTs.$\backslash$n$\backslash$nCONCLUSION: The data suggest that painDETECT assesses pain quality associated with augmented central pain processing in patients with OA. Although developed as a screening questionnaire, painDETECT may also function as a measure of characteristics that indicate augmented central pain processing. Agreement between painDETECT and S-LANSS for pain classification was low, and it is currently unknown which tool may best predict treatment outcome.},
annote = {2 Sp{\o}rgeskema, PainDETECT og S-LANSS, Sammenholt med QST (Pressurepain thresholds, PPTs) Der er st{\o}rk koalation imellem painDETECT og QST men ikke S-LANSS},
author = {Moreton, Bryan J. and Tew, Victoria and {Das Nair}, Roshan and Wheeler, Maggie and Walsh, David A. and Lincoln, Nadina B.},
doi = {10.1002/acr.22431},
file = {::},
isbn = {2151-4658},
issn = {21514658},
journal = {Arthritis Care and Research},
number = {4},
pages = {519--528},
pmid = {25155472},
title = {{Pain phenotype in patients with knee osteoarthritis: Classification and measurement properties of painDETECT and self-report leeds assessment of neuropathic symptoms and signs scale in a cross-sectional study}},
volume = {67},
year = {2015}
}
