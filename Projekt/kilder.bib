@article{Cornelius2015,
abstract = {Osteoarthritis is among the leading causes of persistent pain and disability across the world. A broad array of international studies have confirmed that while total joint replacement is a beneficial and cost-effective treatment for end-stage OA, patients experience tremendous variability in post-operative pain and functional outcomes. Identifying predictors of long-term outcomes following joint replacement would be a crucial step in identifying high-risk patients who could potentially benefit from additional, potentially preventive treatments. To date, a few prior studies have suggested that individual variability in pain sensitivity and pain modulation, measured by quantitative sensory testing (QST), can be used to predict pain ratings after total knee replacement (TKR), but there has been little work targeted at identifying which QST variables are most predictive of persistent post-TKR pain outcomes. Our ongoing multisite study evaluated a total of 103 OA patients that were scheduled for TKR surgery (mean age = 65.8). QST was assessed prior to surgery, and the Brief Pain Inventory (BPI) was used to classify patients into groups with low/ minimal pain, or with clinically significant pain (i.e., scores of {\$} 3/10 on the BPI Pain Severity/Interference scales) at 3-4 months following surgery. QST variables included pressure pain thresholds measured at multiple body sites, heat pain threshold and tolerance, cold pain tolerance and cold pain ratings, temporal summation of mechanical pain, conditioned pain modulation (CPM), and responses to deep-tissue mechanical pain. Results revealed that cold pain after-sensations, a measure of central pain-facilitatory processes, were the single best predictor (p{\textless} .01) of clinically significant post-operative pain at 3-4 months post-surgery. Other commonly-studied QST measures such as pressure pain thresholds were generally non-predictive. These findings highlight the importance of assessing individual variability in dynamic measures of pain modulation, specifically pain-facilitatory processes, when investigating psychophysical predictors of post-operative pain outcomes.},
author = {Cornelius, M and Walker, J and Pejsa, M and Hand, M and Campbell, C and Haythornthwaite, J and Khanuja, P and Sterling, R and Smith, M and Edwards, R},
doi = {10.1016/j.jpain.2015.01.116},
file = {:Volumes/Macintosh HD/Desktop/TEC/(201) Pre-surgical Quantitative Sensory Testing predicts persistent postoperative pain in total knee replacement patients.pdf:pdf},
issn = {1526-5900},
journal = {Journal of Pain},
number = {4 SUPPL. 1},
pages = {S26},
publisher = {Elsevier Ltd},
title = {{Pre-surgical Quantitative Sensory Testing predicts persistent postoperative pain in total knee replacement patients}},
url = {Available from Elsevier in http://linker.worldcat.org/?rft.institution{\_}id=129788{\&}spage=S26{\&}pkgName=ckjournalathens{\&}issn=1526-5900{\&}linkclass=to{\_}article{\&}jKey=15265900{\&}issue=4+SUPPL.+1{\&}provider=elsevier{\&}date=2015-04{\&}aulast=Cornelius+M.{\&}atitle=Pre-surgical+Qu},
volume = {16},
year = {2015}
}
