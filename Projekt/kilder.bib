isbn = {0000000000000},
issn = {0304-3959},
journal = {Pain},
keywords = {cluster analysis,factor analysis,knee pain,osteoarthritis,pain,pain phenotypes,quantitative sensory testing,sensitivity profiles},
pages = {1},
pmid = {27152688},
title = {{Pain sensitivity profiles in patients with advanced knee osteoarthritis}},
url = {http://content.wkhealth.com/linkback/openurl?sid=WKPTLP:landingpage{\&}an=00006396-900000000-99516},
volume = {7036},
