@article{Lewis2015,
abstract = {BACKGROUND: Several studies have identified clinical, psychosocial, patient characteristic, and perioperative variables that are associated with persistent postsurgical pain; however, the relative effect of these variables has yet to be quantified. The aim of the study was to provide a systematic review and meta-analysis of predictor variables associated with persistent pain after total knee arthroplasty (TKA).$\backslash$n$\backslash$nMETHODS: Included studies were required to measure predictor variables prior to or at the time of surgery, include a pain outcome measure at least 3 months post-TKA, and include a statistical analysis of the effect of the predictor variable(s) on the outcome measure. Counts were undertaken of the number of times each predictor was analysed and the number of times it was found to have a significant relationship with persistent pain. Separate meta-analyses were performed to determine the effect size of each predictor on persistent pain. Outcomes from studies implementing uni- and multivariable statistical models were analysed separately.$\backslash$n$\backslash$nRESULTS: Thirty-two studies involving almost 30 000 patients were included in the review. Preoperative pain was the predictor that most commonly demonstrated a significant relationship with persistent pain across uni- and multivariable analyses. In the meta-analyses of data from univariate models, the largest effect sizes were found for: other pain sites, catastrophizing, and depression. For data from multivariate models, significant effects were evident for: catastrophizing, preoperative pain, mental health, and comorbidities.$\backslash$n$\backslash$nCONCLUSIONS: Catastrophizing, mental health, preoperative knee pain, and pain at other sites are the strongest independent predictors of persistent pain after TKA.},
author = {Lewis, G. N. and Rice, D. A. and McNair, P. J. and Kluger, M.},
doi = {10.1093/bja/aeu441},
file = {:Volumes/Macintosh HD/Library/Application Support/Mendeley Desktop/Downloaded/Lewis et al. - 2015 - Predictors of persistent pain after total knee arthroplasty A systematic review and meta-analysis.pdf:pdf},
isbn = {0007-0912},
issn = {14716771},
journal = {British Journal of Anaesthesia},
keywords = {arthroplasty,chronic pain,knee joint,meta-analysis},
number = {4},
pages = {551--561},
pmid = {25542191},
title = {{Predictors of persistent pain after total knee arthroplasty: A systematic review and meta-analysis}},
volume = {114},
year = {2015}
}
